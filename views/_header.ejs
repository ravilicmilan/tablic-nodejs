          <li><a id="nav-login" href="#" style="color:#fff">Login</a></li>
          <li><a id="nav-signup" href="#" style="color:#fff">Sign up</a></li>
        <% } else { %>
          <li style="padding:0 12px; display:flex;align-items:center;color:#fff">Hello, 
            <strong style="margin-left:8px;color:#fff"><%= currentUser %></strong>
          </li>
          <li><a id="nav-logout" href="#" style="color:#fff">Logout</a></li>
        <% } %>
